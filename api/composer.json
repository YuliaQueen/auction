{
    "name": "queendev/auction",
    "type": "project",
    "authors": [
        {
            "name": "Julia Koroleva"
        }
    ],
    "config": {
        "sort-packages": true,
        "process-timeout": 0
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "php-di/php-di": "^7.0",
        "slim/psr7": "^1.7",
        "slim/slim": "^4.14",
        "symfony/console": "^7.1"
    },
    "scripts": {
        "app": "php bin/app.php --ansi",
        "lint": "phplint",
        "psalm": "./vendor/bin/psalm",
        "test": "phpunit --colors=always"
    },
    "require-dev": {
        "overtrue/phplint": "^9.4",
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.26"
    }
}
